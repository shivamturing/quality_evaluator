# ==============================================================================
# LLM Provider Configuration (REQUIRED - choose one)
# ==============================================================================
LLM_PROVIDER=google

# Google Configuration (required if LLM_PROVIDER=google)
GOOGLE_API_KEY=your_google_api_key_here
GOOGLE_MODEL=gemini-2.5-pro

# OpenAI Configuration (required if LLM_PROVIDER=openai)
# OPENAI_API_KEY=your_openai_api_key_here
# OPENAI_MODEL=gpt-4o

# ==============================================================================
# Frontend Configuration (Optional - only needed for web interface)
# ==============================================================================
FRONTEND_PORT=5002
FRONTEND_DEBUG=0
FRONTEND_SECRET_KEY=your_secret_key_here

# Admin Authentication
ADMIN_USER=your_admin_username
ADMIN_PASS=your_admin_password

# RL Gym Integration (Optional - only needed for task fetching from RL Gym)
RL_GYM_API_KEY=your_rl_gym_api_key_here
RL_GYM_BASE_URL=https://rl-gym-advance.turing.com
